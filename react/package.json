{
  "name": "react-ssr",
  "version": "1.0.0",
  "scripts": {
    "build:dev:server": "cross-env NODE_ENV=development webpack --config webpack.server.js --color --watch",
    "build:dev:client": "cross-env NODE_ENV=development webpack --config webpack.client.js --color --watch",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.prod.server.js --color",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.prod.client.js --color",
    "build": "rimraf build && rimraf public && npm run build:server && npm run build:client",
    "dev": "nodemon build/bundle.js",
    "start": "pm2 start build/bundle.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.8.3",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "isomorphic-style-loader": "^5.1.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.2.0",
    "postcss-url": "^10.1.1",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^2.5.2"
  }
}
